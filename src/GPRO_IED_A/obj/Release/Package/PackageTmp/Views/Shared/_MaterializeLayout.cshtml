<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            @Html.Action("HeadPartial", "Shared")
            <div class="right_col" role="main">
                <div class="clearfix"></div>

                @RenderBody()
                <footer style="margin:auto !important">
                    <div style="text-align:center">
                        IED Stand Alone <br />Nhà Phát triển : GPRO COMPANY<br />Email : trivodai@yahoo.com - Điện Thoại : 0918 319714.
                    </div>
                    <div class="clearfix"></div>
                </footer>
            </div>
        </div>
    </div>
    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>
    <div id="loading" style=" display:none">
        <img src="~/Content/Img/loading.gif" />
    </div>
    <style>
        .tab-content {
            padding: 5px;
            background: white;
            border: 1px solid #ddd;
            border-top: none;
            border-bottom-left-radius: 5px;
            border-bottom-right-radius: 5px;
        }

        footer {
            border: 1px solid #ccc;
            margin-top: 10px;
            border-left: none;
            border-right: none;
            background: #172D44;
            font-weight: bold;
            color: #ccc;
        }

        #loading {
            width: 100%;
            height: 100%;
            background: rgba(204, 204, 204, 0.37);
            top: 0 !important;
        }

            #loading img {
                position: fixed;
                top: 35%;
                right: 50%;
            }

        .toggle{
            padding-top:5px !important
        }

        nav {
            background: #EDEDED !important;
        }
        #menu_toggle {
            color: #5A738E !important;
        }
    </style>

    <script>
        NProgress.done();
        $(document).ready(function () {
            $('#menu_toggle').click();
        });
    </script>

</body>
