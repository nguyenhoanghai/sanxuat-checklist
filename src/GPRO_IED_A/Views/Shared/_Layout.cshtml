    <style>
        .phenom {
            border-bottom: 1px solid #d4d6d8;
            /*margin-left: 38px;*/
            min-height: 30px;
            padding: 5px 0;
            position: relative;
            min-height: 40px;
            margin-bottom: 5px;
        }

        .phenom-desc {
            display: inline;
            margin: 0 5px 0 0;
            word-wrap: break-word;
            font-size: 13px;
            line-height: initial;
        }

            .phenom-desc span {
                font-weight: 700;
            }

        .phenom-meta {
            display: inline-block;
            font-size: 9pt;
            margin: 0;
            min-width: 110px;
            padding-left: 29px;
        }

        .phenom-desc {
            display: table-cell !important;
            padding-right: 15px;
        }

        .phenom-close {
            position: absolute;
            top: 0;
            right: 0;
        }

            .phenom-close .fa-times {
                color: #ccc;
            }

                .phenom-close .fa-times:hover {
                    color: #888;
                }


        .phenom-meta a.date {
            text-decoration: none;
        }

        .quiet, .quiet a {
            color: #8c8c8c;
        }

        .member {
            background-color: #D6DADC;
            border-radius: 3px;
            color: #4d4d4d;
            display: block;
            float: left;
            height: 30px;
            margin: 0 4px 4px 0;
            overflow: hidden;
            position: relative;
            width: 30px;
            user-select: none;
            z-index: 0;
        }

        .member-avatar {
            height: 30px;
            width: 30px;
        }

        .action-comment {
            background-color: #fff;
            border-radius: 3px;
            box-shadow: 0 1px 2px rgba(0,0,0,.23);
            -moz-box-sizing: border-box;
            box-sizing: border-box;
            clear: both;
            cursor: pointer;
            display: inline-block;
            margin: 6px 2px 6px 0;
            overflow: hidden;
            text-overflow: ellipsis;
            max-width: 100%;
        }

        .markeddown {
            word-wrap: break-word;
        }

        .action-comment .current-comment {
            padding: 9px 11px;
        }

        .action-comment p:last-child {
            margin-bottom: 0;
        }

        .action-comment p {
            text-overflow: ellipsis;
            word-wrap: break-word;
            line-height: 18px;
        }

        .phenom .comment-box {
            display: none;
        }

        .comment-box {
            position: relative;
        }

        .comment-box-input, .comment-box-input:active, .comment-box-input:focus, .comment-box-input:hover {
            border: 0;
            border-bottom: solid 30px transparent;
        }

        .comment-box-input {
            background: 0 0;
            height: 75px;
            margin: 0;
            min-height: 75px;
            padding: 9px 11px 0;
            width: 100%;
            resize: none;
        }

        .comment-box-options {
            position: absolute;
            bottom: 6px;
            right: 6px;
        }

        .comment-box-options-item {
            border-radius: 3px;
            float: left;
            height: 18px;
            margin-left: 4px;
            padding: 6px;
        }

        .navbar-nav .open .dropdown-menu.msg_list {
            width: 500px !important;
        }

        ul.msg_list li {
            padding: 0 !important;
        }

            ul.msg_list li:last-child {
                margin-bottom: 6px;
            }

        #notify_box {
            position: fixed;
            bottom: 10px;
            right: 10px;
            background: white;
            border-radius: 5px;
            border: 1px solid #bbb;
            max-width: 450px;
            display: none;
            z-index: 9999;
        }

        .no-title {
            padding: 5px 15px;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
            font-weight: bold;
            border-color: #c9c9c9;
            color: #5f5f5f;
            background-color: #d6d5d4;
            background-image: linear-gradient(#fff,#d6d5d4);
            text-align: center;
        }

        .no-content {
            padding: 5px 15px;
            background: url('/Images/alert.png') no-repeat 15px center;
            padding-left: 65px;
            max-height: 280px;
            overflow: auto;
        }


        #notify_box ul {
            padding: 5px 0;
            margin: 0;
            border-bottom: 1px solid #bbb;
        }

            #notify_box ul li {
                list-style: none;
            }

        #icon-speaker,
        #icon-cl {
            font-size: 18px;
            float: right;
            padding: 0 3px;
        }

        #icon-cl {
            color: #E60404;
        }

        .modalsmall .col_left,
        .modalsmall .col-right {
            float: none !important;
            width: 100% !important
        }
    </style>
